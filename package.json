{
  "name": "astronot",
  "type": "module",
  "version": "0.5.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run sync:all && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "sync": "rimraf src/pages/posts/* && node src/astronot.js",
    "sync:published": "rimraf src/pages/posts/* && node src/astronot.js --published",
    "sync:projects": "rimraf src/pages/projects/*.mdx && ([ -d 'src/pages/projects' ] || mkdir src/pages/projects) && node src/scripts/generate-project-files.js",
    "sync:testimonials": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-testimonials-data.js",
    "sync:hero": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-hero-data.js",
    "sync:services": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-services-data.js",
    "sync:categories": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-categories-data.js",
    "sync:social-links": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-social-links-data.js",
    "sync:work-experience": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-work-experience-data.js",
    "sync:author": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-author-data.js",
    "sync:about": "([ -d 'src/data' ] || mkdir src/data) && node src/scripts/generate-about-data.js",
    "sync:images": "node src/scripts/sync-notion-images.js",
    "sync:images:local": "node src/scripts/sync-notion-images.js --local",
    "sync:images:s3": "node src/scripts/sync-notion-images.js --s3",
    "sync:notion": "node src/scripts/sync-all-notion.js",
    "sync:all": "npm run sync:notion",
    "generate": "rimraf dist/** && ([ -d 'dist' ] || mkdir dist) && ([ -d 'dist/images' ] || mkdir dist/images) && ([ -d 'src/pages/posts' ] || mkdir src/pages/posts) && ([ -d 'src/images' ] || mkdir src/images) && ([ -d 'src/images/posts' ] || mkdir src/images/posts) && npm run sync:all && astro build"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.1.0",
    "@astrojs/react": "^4.2.1",
    "@astrojs/svelte": "^7.0.5",
    "@astrojs/tailwind": "^6.0.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@notionhq/client": "^2.2.15",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "astro": "~5.4.2",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "flowbite": "^2.3.0",
    "flowbite-svelte": "^0.48.4",
    "flowbite-svelte-blocks": "^1.1.1",
    "flowbite-typography": "^1.0.3",
    "image-type": "^5.2.0",
    "lodash": "^4.17.21",
    "marked": "^15.0.7",
    "mime-types": "^2.1.35",
    "nanostores": "^0.10.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "notion-to-md": "^3.1.1",
    "prettier-plugin-svelte": "^3.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "read-chunk": "^4.0.3",
    "reading-time": "^1.5.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^5.0.7",
    "sharp": "^0.33.4",
    "svelte": "^5.20.1",
    "svelte-carousel": "^1.0.25",
    "tailwind-merge": "^2.3.0",
    "tinycolor2": "^1.6.0",
    "vite": "^6.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "flowbite-svelte-icons": "^2.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prettier-plugin-toml": "^2.0.2",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animated": "^1.0.1"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-toml"
    ]
  }
}
